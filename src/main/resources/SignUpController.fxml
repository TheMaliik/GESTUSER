<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">
   <children>
      <AnchorPane layoutX="409.0" layoutY="100.0" prefHeight="450.0" prefWidth="346.0">
         <children>
            <TextField fx:id="nomField" layoutY="76.0" prefHeight="30.0" prefWidth="260.0" promptText="Entrez votre nom de famille" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
            <TextField fx:id="prenomField" layoutY="119.0" prefHeight="30.0" prefWidth="260.0" promptText="Entrez votre prénom" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
            <TextField fx:id="emailField" layoutX="50.4" layoutY="162.0" prefHeight="30.0" prefWidth="260.0" promptText="Entrez votre e-mail" AnchorPane.leftAnchor="50.4" AnchorPane.rightAnchor="50.4" />
            <TextField fx:id="ageField" layoutX="50.4" layoutY="205.0" prefHeight="30.0" prefWidth="260.0" promptText="Entrez votre âge" AnchorPane.leftAnchor="50.4" AnchorPane.rightAnchor="50.4" />
            <ComboBox fx:id="roleComboBox" layoutX="50.4" layoutY="248.0" prefHeight="30.0" prefWidth="260.0" promptText="Sélectionnez un rôle" AnchorPane.leftAnchor="50.4" AnchorPane.rightAnchor="50.4" />
            <TextField fx:id="motDePasseField" layoutX="50.4" layoutY="291.0" prefHeight="30.0" prefWidth="260.0" promptText="Entrez votre mot de passe" AnchorPane.leftAnchor="50.4" AnchorPane.rightAnchor="50.4" />
            <TextField fx:id="confMotDePasse" layoutX="51.2" layoutY="334.0" prefHeight="30.0" prefWidth="260.0" promptText="Confirmez le mot de passe" AnchorPane.leftAnchor="51.2" AnchorPane.rightAnchor="49.6" />
            <Button fx:id="Badd" layoutX="59.0" layoutY="392.0" mnemonicParsing="false" onAction="#add" prefHeight="30.0" prefWidth="100.0" text="Soumettre" />
            <Button fx:id="generatePasswordButton" layoutX="380.0" layoutY="293.0" onAction="#generatePassword" prefHeight="2.0" prefWidth="2.0" text=".." />
            <Text layoutX="36.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Profitez pleinement de votre expérience avec nous">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#a60000" layoutX="107.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="INSCRIPTION" AnchorPane.leftAnchor="106.4" AnchorPane.rightAnchor="106.4">
               <font>
                  <Font size="35.0" />
               </font>
            </Text>
            <Button fx:id="cancel" graphicTextGap="10.0" layoutX="188.0" layoutY="392.0" mnemonicParsing="false" onAction="#cancel" prefHeight="30.0" prefWidth="100.0" text="Annuler" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <ImageView fitHeight="600.0" fitWidth="401.0">
         <image>
            <Image url="@/Assets/Blue.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="270.0" fitWidth="308.0" layoutX="47.0" layoutY="165.0">
         <image>
            <Image url="@/Assets/logoo.png" />
         </image>
      </ImageView>
   </children>
   <stylesheets>
      <URL value="@/SignUp.css" />
   </stylesheets>
</AnchorPane>
